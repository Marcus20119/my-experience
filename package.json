{
  "name": "bespoke-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx src/ --ext .json src/shared/locales --fix",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "pretty": "prettier . --write",
    "knip": "knip",
    "codegen": "sh scripts/codegen.sh",
    "i18n-check-unused": "node scripts/i18nScan.cjs",
    "check-types": "tsc --noEmit --pretty",
    "check-types:watch": "tsc --noEmit --watch",
    "lint-staged": "lint-staged",
    "build:analyze": "tsc && DISABLE_ESLINT_PLUGIN=true ANALYZE=true vite build",
    "remove-source-maps": "rimraf build/**/*.js.map",
    "postinstall": "husky install",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "test:units": "vitest run tests/unit",
    "test:integrations": "vitest run tests/integrations",
    "test:watch": "vitest",
    "generate:svg-icons": "node scripts/generateSvgIcons.cjs"
  },
  "dependencies": {
