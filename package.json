    "@tanstack/react-query": "^5.54.1",
    "@tanstack/react-query-devtools": "^5.54.1",
    "@vitejs/plugin-react": "^4.3.3",
    "antd": "^5.22.7",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "graphql": "^16.8.1",
    "iconsax-react": "^0.0.8",
    "konva": "^9.3.16",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-konva": "^18.2.10",
